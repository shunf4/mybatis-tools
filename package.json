{
    "name": "mybatis-tools",
    "displayName": "mybatis-tools",
    "description": "tools for mybatis",
    "publisher": "nikolazhang",
    "version": "0.0.1",
    "repository": {
        "url": "https://gitee.com/NikolaZhang/mybatis-tools.git",
        "type": "git"
    },
    "engines": {
        "vscode": "^1.46.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:mybatis-tools.hello",
        "onCommand:mybatis-tools.config",
        "onCommand:mybatis-tools.jumper"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "mybatis-tools.config",
                "title": "mybatis-tools config"
            },
            {
                "command": "mybatis-tools.hello",
                "title": "mybatis hello page"
            },
            {
                "command": "mybatis-tools.jumper",
                "title": "mybatis jumper"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.0",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.46.0",
        "@typescript-eslint/eslint-plugin": "^4.1.1",
        "@typescript-eslint/parser": "^4.1.1",
        "eslint": "^7.9.0",
        "glob": "^7.1.6",
        "mocha": "^8.1.3",
        "typescript": "^4.0.2",
        "vscode-test": "^1.4.0"
    },
    "dependencies": {
        "fast-xml-parser": "^4.0.0-beta.8"
    }
}